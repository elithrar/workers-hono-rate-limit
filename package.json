{
	"name": "@elithrar/workers-hono-rate-limit",
	"version": "0.5.1",
	"type": "module",
	"description": "Hono-compatible middleware for rate limiting requests with Cloudflare Workers.",
	"scripts": {
		"test": "vitest run",
		"build": "npx unbuild",
		"publish": "npx unbuild"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/elithrar/workers-hono-rate-limit.git"
	},
	"homepage": "https://github.com/elithrar/workers-hono-rate-limit",
	"peerDependencies": {
		"hono": ">=4.0.0"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.11.1",
		"@cloudflare/workers-types": "^4.20251225.0",
		"hono": "^4.11.2",
		"unbuild": "^3.6.1",
		"vitest": "^3.2.0",
		"wrangler": "^4.54.0"
	}
}

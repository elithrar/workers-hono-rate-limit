{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"types": ["@cloudflare/workers-types"],
		"rootDir": "./src",
		"outDir": "./dist"
	},
	"include": ["src/**/*.ts"]
}
